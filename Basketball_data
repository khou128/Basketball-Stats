      "source": [
        "from scipy.stats.mstats import winsorize\n",
        "\n",
        "# Assuming df is your DataFrame and 'column_name' is the column containing the data with extreme values\n",
        "# For example, let's say you want to winsorize the 'value' column\n",
        "winsorized_values = winsorize(df_1['ts_pct'], limits=[0.05, 0.05])  # Winsorize values below the 5th percentile and above the 95th percentile\n",
        "\n",
        "# Replace the original column with the winsorized values\n",
        "df_1['ts_pct'] = winsorized_values"
      ],
      "metadata": {
        "id": "y9f-h56ZY3IV"
      },
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from scipy.stats.mstats import winsorize\n",
        "\n",
        "# Assuming df is your DataFrame and 'column_name' is the column containing the data with extreme values\n",
        "# For example, let's say you want to winsorize the 'value' column\n",
        "winsorized_values = winsorize(df_1['ast_pct'], limits=[0.05, 0.05])  # Winsorize values below the 5th percentile and above the 95th percentile\n",
        "\n",
        "# Replace the original column with the winsorized values\n",
        "df_1['ast_pct'] = winsorized_values"
      ],
